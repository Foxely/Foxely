

myClass :: class
{
    init()
    {
        print "The class has been init\n";
        this.a = 5;
    }

    operator +(n)
    {
        return this.a + n;
    }

    operator -(n)
    {
        return this.a - n;
    }

    operator *(n)
    {
        return this.a * n;
    }

    operator /(n)
    {
        return this.a / n;
    }
}


c := myClass();
print "c + 2 = %\n", c + 2;
print "c - 2 = %\n", c - 2;
print "c * 2 = %\n", c * 2;
print "c / 2 = %\n", c / 2;