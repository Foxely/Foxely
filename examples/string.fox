

word := "Hello World";

print "%\n", word.length(); // length of the string
// 11

print "%\n", word.count("l"); // count how many times l is in the string
// 3

print "%\n", word.find("H"); // find the word H in the string
// 0

print "%\n", word.split(" "); // Split on whitespace
// ['Hello', 'World']

word.replace("Hello", "Goodbye");

print "%\n", word;
// Goodbye World