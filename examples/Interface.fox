

Vehicle :: interface
{
    start_engine();
}

Car :: class < Vehicle
{
    start_engine()
    {
        print "Car start the engine\n";
    }
}

Truck :: class < Vehicle
{
    start_engine()
    {
        print "Truck start the engine\n";
    }
}

c: var = Car();
c2: var = Truck();

c.start_engine();
c2.start_engine();