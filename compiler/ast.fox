BinaryExpression :: class < Expression
{
    init(left, right)
    {
        this.left = left;
        this.right = right;
    }

	visit()
    {
		print "Binary";
		this.left.visit();
		this.right.visit();
    }
}
  
SumExpression :: class < BinaryExpression
{
	visit()
    {
		// print " ";
		// print "+";
		this.left.visit();
		this.right.visit();
        emit(1, "ADD $10 $20");
    }
}

SubtractionExpression :: class < BinaryExpression
{
	visit()
    {
		print " ";
		print "-";
		this.left.visit();
		this.right.visit();
    }
}

MultiplicationExpression :: class < BinaryExpression
{
	visit()
    {
		print " ";
		print "*";
		this.left.visit();
		this.right.visit();
    }
}

DivisionExpression :: class < BinaryExpression
{
	visit()
    {
		print " ";
		print "/";
		this.left.visit();
		this.right.visit();
    }
}

