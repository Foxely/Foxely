ExpressionPrintingVisitor :: class
{
	PrintLiteral(literal)
	{
		print "%\n", literal.Value;
	}
	
	PrintAddition(addition)
	{
		leftValue = addition.Left.GetValue();
		rightValue = addition.Right.GetValue();
		var sum = addition.GetValue();
		Console.WriteLine("{0} + {1} = {2}", leftValue, rightValue, sum);
	}
}